{
  "name": "react-native-web-uikit",
  "description": "UI Kit for react-native and react-native-web",
  "engines": {
    "node": "~10.14.0",
    "npm": "~6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:trucknet-io/react-native-web-uikit.git"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.lib.js",
  "version": "2.7.3",
  "private": false,
  "scripts": {
    "start": "npm run find-stories && node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "commit": "commit-prompt",
    "precommit": "lint-staged && npm run lint",
    "lint": "npm run lint:tsc && npm run lint:tslint",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "lint:tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "lint:prettier": "prettier --write \"{src,definitions,__tests__}/**/*.ts*\"",
    "test": "jest --detectOpenHandles",
    "test:unit": "jest --detectOpenHandles --testPathPattern \"src/.+spec.tsx?$\"",
    "test:e2e": "jest --detectOpenHandles --testPathPattern \"__tests__/.+.tsx?\"",
    "test:ci": "jest --detectOpenHandles --coverage",
    "build": "rimraf dist/ && tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "find-stories": "rnstl",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "prepublishOnly": "npm run build",
    "postrelease": "npm publish",
    "release": "./extra/release.sh"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.story.tsx",
      "outputFile": "./rn_storybook/storyLoader.js"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-info": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addon-notes": "^5.0.3",
    "@storybook/addon-ondevice-knobs": "^5.2.6",
    "@storybook/addon-ondevice-notes": "^5.2.6",
    "@storybook/addons": "^5.0.3",
    "@storybook/react": "^5.2.6",
    "@storybook/react-native": "^5.2.6",
    "@testing-library/react-native": "^5.0.1",
    "@types/jest": "23.3.10",
    "@types/react": "16.7.18",
    "@types/react-native": "0.57.23",
    "@types/react-test-renderer": "16.8.3",
    "@types/webpack": "^4.39.2",
    "app-icon": "^0.6.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-react-app": "^7.0.2",
    "babel-runtime": "^6.26.0",
    "core-js": "^2.6.10",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "jest": "23.6.0",
    "jetifier": "^1.6.3",
    "markdown-loader": "^5.1.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier-config-trucknet-io": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-app-rewired": "^2.1.1",
    "react-art": "16.8.3",
    "react-content-loader": "^4.2.2",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "16.8.3",
    "react-native": "0.59.3",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-modal": "^11.3.1",
    "react-native-popup-menu": "^0.15.6",
    "react-native-shimmer-placeholder": "^1.0.35",
    "react-native-storybook-loader": "^1.8.1",
    "react-native-svg": "^9.4.0",
    "react-native-svg-web": "^1.0.1",
    "react-native-web": "^0.10.1",
    "react-native-web-linear-gradient": "^1.0.5",
    "react-native-web-webview": "^0.2.8",
    "react-native-webview": "^6.8.1",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "16.8.3",
    "rimraf": "^2.6.3",
    "storybook-addon-smart-knobs": "^6.0.0",
    "storybook-readme": "^5.0.8",
    "trucknet-commit": "^1.4.0",
    "ts-jest": "^24.2.0",
    "tscpaths": "0.0.9",
    "tslint-config-trucknet-io": "^1.3.0",
    "typescript": "3.7.3"
  },
  "peerDependencies": {
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-native": "0.59.3",
    "react-art": "16.8.3",
    "react-native-web": "^0.10.0",
    "react-native-web-linear-gradient": "^1.0.5",
    "react-native-svg-web": "^1.0.1",
    "react-native-linear-gradient": "^1.0.5",
    "react-native-svg": "^9.3.5",
    "modal-react-native-web": "^0.2.0",
    "react-native-modal": "^11.3.1",
    "react-native-popup-menu": "^0.15.6",
    "react-native-web-webview": "^0.2.8",
    "react-native-webview": "^6.8.1",
    "react-content-loader": "^4.2.2",
    "react-native-shimmer-placeholder": "^1.0.35"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "keywords": [
    "react-native-uikit",
    "react-native-web-uikit",
    "react-native-modal",
    "react-native-drawer",
    "react-native-web-modal",
    "react-native-web-drawer",
    "react-native-web-storybook-uikit"
  ]
}
